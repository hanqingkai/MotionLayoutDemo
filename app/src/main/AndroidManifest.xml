<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.motionlayoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MotionLayoutDemo">
        <activity
            android:name=".EasterEggsActivity"
            android:exported="false" />
        <activity
            android:name=".ConstraintFlowActivity"
            android:exported="false" />
        <activity
            android:name=".CoordinatorlayoutActivity"
            android:exported="false" />
        <activity
            android:name=".ViewpagerActivity"
            android:exported="false" />
        <activity
            android:name=".CodeControlActivity"
            android:exported="false" />
        <activity
            android:name=".DemoListActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>