<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <OnClick
            motion:clickAction="toggle"
            motion:targetId="@+id/fabCenter" />
        <!--     初始状态   -->
        <KeyFrameSet>


            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab0" />

            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab30" />
            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab90" />
            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab135" />
            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab180" />
            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab225" />

            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab270" />


            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab315" />


            <KeyAttribute
                android:alpha="0"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition="0"
                motion:motionTarget="@id/fab360" />


        </KeyFrameSet>
        <!--结束状态-->
        <KeyFrameSet>
            <KeyAttribute
                android:rotation="720"
                android:scaleX="2"
                android:scaleY="2"
                motion:framePosition="100"
                motion:motionTarget="@id/fabCenter">
                <CustomAttribute
                    motion:attributeName="tint"
                    motion:customColorValue="@color/design_default_color_error" />
            </KeyAttribute>
            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab0" />

            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab30" />

            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab90" />
            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab135" />
            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab180" />
            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab225" />

            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab270" />


            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab315" />


            <KeyAttribute
                android:alpha="1"
                android:rotation="720"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="100"
                motion:motionTarget="@id/fab360" />
        </KeyFrameSet>

    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/fabCenter"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/fab0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab30"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab90"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />

        <Constraint
            android:id="@+id/fab135"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />

        <Constraint
            android:id="@+id/fab180"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab225"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab270"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab315"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />
        <Constraint
            android:id="@+id/fab360"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="0dp" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/fab0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="0"
            motion:layout_constraintCircleRadius="100dp" />
        <Constraint
            android:id="@+id/fab30"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="45"
            motion:layout_constraintCircleRadius="100dp" />

        <Constraint
            android:id="@+id/fab90"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="90"
            motion:layout_constraintCircleRadius="100dp" />

        <Constraint
            android:id="@+id/fab135"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="135"
            motion:layout_constraintCircleRadius="100dp" />

        <Constraint
            android:id="@+id/fab180"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="180"
            motion:layout_constraintCircleRadius="100dp" />
        <Constraint
            android:id="@+id/fab225"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="225"
            motion:layout_constraintCircleRadius="100dp" />

        <Constraint
            android:id="@+id/fab270"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="270"
            motion:layout_constraintCircleRadius="100dp" />
        <Constraint
            android:id="@+id/fab315"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="315"
            motion:layout_constraintCircleRadius="100dp" />
        <Constraint
            android:id="@+id/fab360"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintCircle="@id/fabCenter"
            motion:layout_constraintCircleAngle="360"
            motion:layout_constraintCircleRadius="100dp" />
    </ConstraintSet>
</MotionScene>